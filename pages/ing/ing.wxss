.topInfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100vw;
  margin-bottom: 50rpx;
  justify-content: space-around;
}

.missionstyle {
  display: flex;
  flex-direction: column;
  width: 150rpx;
  align-items: center;
}

.playstyle {
  display: flex;
  flex-direction: column;
  width: 120rpx;
  align-items: center;
}

.topImage1 {
  height: 150rpx;
  width: 100rpx;
}

.topImage2 {
  height: 150rpx;
  width: 100rpx;
}

.bar {
  width: 710rpx;
  margin-top: 20rpx;
  height:80rpx;
  background-color: rgba(235, 76, 51, 0.801);
  border-radius: 50rpx;
  box-shadow: 5rpx 5rpx 20rpx 2rpx rgba(235, 76, 51, 0.568);
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.barimage {
  height:100rpx;
  width: 100vw;
}

.barname {
  display: block;
  height: 50rpx;
  width: 200rpx;
  margin-left: 50rpx;
  color: white;
  font-weight: bold;
}

.barinfo {
  width: 100%;
  padding-top: 20rpx;
}

.barnum {
  margin-top: -50rpx;
