  width: 710rpx;
  margin-top: 20rpx;
  border-radius: 60rpx;
  box-shadow: 5rpx 5rpx 20rpx 2rpx whitesmoke;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.missionnameview {
